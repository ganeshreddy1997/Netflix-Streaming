package io.springboot2.x.entity;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table
public class NetflixPlan {
	
	private String planId;
	private String planName;
	private String priceForMonth;
	private String featuresProvide;
	public String getPlanId() {
		return planId;
	}
	public void setPlanId(String planId) {
		this.planId = planId;
	}
	public String getPlanName() {
		return planName;
	}
	public void setPlanName(String planName) {
		this.planName = planName;
	}
	public String getPriceForMonth() {
		return priceForMonth;
	}
	public void setPriceForMonth(String priceForMonth) {
		this.priceForMonth = priceForMonth;
	}
	public String getFeaturesProvide() {
		return featuresProvide;
	}
	public void setFeaturesProvide(String featuresProvide) {
		this.featuresProvide = featuresProvide;
	}
	
	

}
